{
  "name": "bolt-on-heroku",
  "version": "1.0.0",
  "description": "Bolt app which runs on Heroku",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "local": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seratch/bolt-on-heroku.git"
  },
  "keywords": [
    "Slack",
    "Bolt",
    "Heroku"
  ],
  "author": "Kazuhiro Sera @seratch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seratch/bolt-on-heroku/issues"
  },
  "homepage": "https://github.com/seratch/bolt-on-heroku#readme",
  "dependencies": {
    "@msgpack/msgpack": "^2.3.0",
    "@slack/bolt": "^2.2.3",
    "@slack/types": "^1.10.0",
    "@slack/web-api": "^5.13.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "<13",
    "@types/node-fetch": "^2.5.7",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "image-data-uri": "^2.0.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
